# Environment variables for Rules Engine Frontend

# API Configuration
VITE_API_BASE_URL=https://rules-engine.local.dev/api/v1
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME="Rules Engine"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Carrefour Business Rules Management System"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# Authentication
VITE_AUTH_TOKEN_KEY=auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=refresh_token

# External Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Development
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost
